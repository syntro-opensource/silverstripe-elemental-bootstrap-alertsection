{
    "name": "syntro/silverstripe-elemental-bootstrap-alertsection",
    "description": "A bootsttrap based alert section",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "dnadesign/silverstripe-elemental": "^4",
        "buckleshusky/fontawesomeiconpicker": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Syntro\\BootstrapAlertsection\\": "src/",
            "Syntro\\BootstrapAlertsection\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
